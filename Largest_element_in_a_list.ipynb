    "def getmax(l):\n",
    "    if not l:\n",
    "        return none\n",
    "    else:\n",
    "        res = l[0]\n",
    "        for i in range(1,len(l)):\n",
    "            if l[i]>res:\n",
    "                res=l[i]\n",
    "        return res"
   ]
